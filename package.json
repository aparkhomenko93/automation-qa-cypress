{
  "devDependencies": {
    "@faker-js/faker": "^10.1.0",
    "cypress": "^15.5.0",
    "mochawesome": "^7.1.4",
    "mochawesome-merge": "^5.0.0",
    "mochawesome-report-generator": "^6.3.2"
  },
  "scripts": {
    "test:v1": "cypress run --config-file cypress.config.js",
    "test:v2": "cypress run --config-file cypress.config.v2.js",
    "merge-reports": "npx mochawesome-merge \"cypress/results/*.json\" > mochawesome.json",
    "generate-report": "npx marge mochawesome.json",
    "report": "npm run merge-reports && npm run generate-report"
  },
  "dependencies": {
    "cypress-plugin-api": "^2.11.2"
  }
}
